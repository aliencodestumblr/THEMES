   var color = '#acd7bb'; // color of play button (hex) 
   $('.soundcloud_audio_player').each(function(){
       $(this).attr({ src: $(this).attr('src').split('&')[0] + '&amp;liking=false&amp;sharing=false&amp;auto_play=false&amp;show_comments=false&amp;continuous_play=false&amp;buying=false&amp;show_playcount=false&amp;show_artwork=false&amp;origin=tumblr&amp;color=' + color.split('#')[1], height: 116, width: '100%' });
   });
});
</script> 

<script>
(() => {
  const root   = document.documentElement;
  const btn    = document.getElementById('theme-toggle');
  const key    = 'theme-preference';

  // 1) Determine starting theme: saved -> system -> light
  const getPref = () => localStorage.getItem(key) ||
    (window.matchMedia('(prefers-color-scheme: dark)').matches ? 'dark' : 'light');

  const apply = (mode) => {
    root.setAttribute('data-theme', mode);
    if (btn) btn.setAttribute('aria-pressed', mode === 'dark');
  };

  apply(getPref());

  // 2) Toggle on click
  btn && btn.addEventListener('click', () => {
    const next = (root.getAttribute('data-theme') === 'dark') ? 'light' : 'dark';
    apply(next);
    localStorage.setItem(key, next);
  });

  // 3) Optional: react to system changes (only if user hasn’t overridden)
  window.matchMedia('(prefers-color-scheme: dark)').addEventListener('change', (e) => {
    if (!localStorage.getItem(key)) apply(e.matches ? 'dark' : 'light');
  });
})();
</script>


<script>feather.replace()</script>

<script>
(() => {
  const toggle = document.getElementById('search-toggle');
  const panel  = document.getElementById('search-panel');
  if (!toggle || !panel) return;

  const inner    = panel.querySelector('.search-inner');
  const closeBtn = panel.querySelector('.search-close');
  const input    = panel.querySelector('input[type="text"], input[type="search"]');

  const isOpen = () => panel.classList.contains('open');
  const setAria = (open) => {
    toggle.setAttribute('aria-expanded', open ? 'true' : 'false');
    panel.setAttribute('aria-hidden', open ? 'false' : 'true');
  };

  // No scroll locking. No padding. No body position changes.

  const onKeydown = (e) => { if (e.key === 'Escape') close(); };
  const onDocClick = (e) => {
    if (!isOpen()) return;
    const inside = inner && inner.contains(e.target);
    const onToggle = toggle.contains(e.target);
    if (!inside && !onToggle) close();
  };

  function open(){
    if (isOpen()) return;
    panel.classList.add('open');
    setAria(true);
    // focus input after animation tick
    setTimeout(() => (input || inner || panel).focus({ preventScroll:true }), 10);
    document.addEventListener('keydown', onKeydown);
    document.addEventListener('click', onDocClick, { capture:true });
  }

  function close(){
    if (!isOpen()) return;
    panel.classList.remove('open');
    setAria(false);
    toggle.focus({ preventScroll:true });
    document.removeEventListener('keydown', onKeydown);
    document.removeEventListener('click', onDocClick, { capture:true });
  }

  toggle.addEventListener('click', (e) => { e.preventDefault(); isOpen() ? close() : open(); });
  closeBtn?.addEventListener('click', (e) => { e.preventDefault(); close(); });
  inner?.addEventListener('click', (e) => e.stopPropagation());

  setAria(isOpen());
})();
</script>

<script>
(() => {
  const BTN = document.getElementById('controls-toggle'); // your .cc button
  if (!BTN) return;

  const KEY = 'controls-open';

  const apply = (on) => {
    document.body.classList.toggle('controls-open', on);
  };

  // Default CLOSED unless user saved "open"
  let saved = null;
  try { saved = localStorage.getItem(KEY); } catch(_) {}
  const startOpen = (saved === '1');  // only open if explicitly saved
  apply(startOpen);

  BTN.addEventListener('click', (e) => {
    e.preventDefault();
    const next = !document.body.classList.contains('controls-open');
    apply(next);
    try { localStorage.setItem(KEY, next ? '1' : '0'); } catch(_) {}
  });
})();
</script>



</body>
</html>
