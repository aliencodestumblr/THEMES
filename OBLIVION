<script>
  (function () {
    var toggle = document.querySelector('#bfy-sidebar .bfy-toggle');
    var panel  = document.querySelector('#bfy-sidebar .bfy-panel');
    if (!toggle || !panel) return;

    toggle.addEventListener('click', function () {
      var open = toggle.getAttribute('aria-expanded') === 'true';
      toggle.setAttribute('aria-expanded', String(!open));
      panel.classList.toggle('is-open', !open);   // let CSS handle the animation
    });
  })();
</script>

<script>
// minimal soundcloud player © shythemes.tumblr
