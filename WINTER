{/block:IndexPage}
<script src="https://static.tumblr.com/wgg6svp/OoTofxa0c/unnest.min.js"></script>

<script>
$(document).ready(function(){
$('.photo-slideshow').pxuPhotoset({
lightbox: true,
rounded: false,
gutter: '1px',
photoset: '.photo-slideshow',
photoWrap: '.photo-data',
photo: '.pxu-photo'
});
$(function(){ $('.posts').unnest({ 
yourCaption:".caption",
wrapName:".tumblr_parent",
newCaptionUsername:true,
originalPostCaptionUsername:true, 
tumblrAvatars:false,  
tumblrAvatarsName:".tumblr_avatar",  
usernameColon:false 
});}) 
{block:IndexPage}
var $container = $('#posts');
$container.masonry({ itemSelector: '.posts' });
$container.imagesLoaded(function(){
$container.masonry();
$container.find('.posts').animate({ opacity: 1, zIndex: 1 });
}); 
$container.infinitescroll({
itemSelector: '.posts',
navSelector: '#pagination',
nextSelector: '#next',
loadingImg: '',
loadingText: '<em></em>',
bufferPx: 1000
},
function(newElements) {
var $newElems = $( newElements ).css({ opacity: 0 });
$newElems.unnest({
yourCaption:".caption",
wrapName:".tumblr_parent",
newCaptionUsername:true,
originalPostCaptionUsername:true, 
tumblrAvatars:false,
tumblrAvatarsName:".tumblr_avatar",  
usernameColon:false });
var $newElemsIDs = $newElems.map(function(){
return this.id;
}).get();
Tumblr.LikeButton.get_status_by_post_ids($newElemsIDs);
var $newElems = $( newElements );
$newElems.find('.photo-slideshow').pxuPhotoset({
lightbox: true,
rounded: false,
gutter: '1px',
photoset: '.photo-slideshow',
photoWrap: '.photo-data',
photo: '.pxu-photo'
},
function(){
$container.masonry();
});
$newElems.imagesLoaded(function(){
$container.masonry( 'appended', $newElems );
$newElems.animate({ opacity: 1, zIndex: 1 });
resizeVideos();
});
}); 
{/block:IndexPage}
});
</script>
  
{block:ifnotpagination} 
{block:IndexPage}
{block:Pagination}
<div id="pagination">
{block:NextPage}
<a id="next" href="{NextPage}"></a>
{/block:NextPage}
{block:PreviousPage}
<a href="{PreviousPage}"></a>
{/block:PreviousPage}
</div>
{/block:Pagination}
{/block:IndexPage}
{/block:ifnotpagination} 
</div> 
</div> 
 
<script>

$(document).ready(function(){

  var accentColor = '#acd7bb';   
  var playerHeight = 116;       

  $('.soundcloud_audio_player').each(function(){

    let baseSrc = $(this).attr('src').split('&')[0];

    let params = [
      'liking=false',
      'sharing=false',
      'auto_play=false',
      'show_comments=false',
      'continuous_play=false',
      'buying=false',
      'show_playcount=false',
      'show_artwork=false',
      'hide_related=true',
      'origin=tumblr',
      'color=' + accentColor.replace('#','')
    ].join('&');

    $(this).attr({
      src: baseSrc + '&' + params,
      height: playerHeight,
      width: '100%',
      loading: 'lazy'
    });

  });

});

</script>

