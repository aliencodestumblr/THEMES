<script>feather.replace()</script>

<script>
(function(){
  if (document.getElementById("pixie-layer")) return;

  var layer = document.createElement("div");
  layer.id = "pixie-layer";
  layer.style.cssText =
    "position:fixed;inset:0;pointer-events:none;z-index:999999999;overflow:hidden;";
  document.body.appendChild(layer);

  // soft fairy palette (white + blush pink)
  var COLORS = [
    "{color:Cursor Plus}",   
    "{color:Cursor Sparkle 1}",   
    "{color:Cursor Sparkle 2}"    
  ];

  function pick(arr){
    return arr[Math.floor(Math.random()*arr.length)];
  }

  function spawn(x,y){
    var el = document.createElement("div");
    el.textContent = "+";

    var color = pick(COLORS);
    var size = 16 + Math.random()*4; // 16–20px

    el.style.cssText =
      "position:absolute;" +
      "left:"+x+"px;top:"+y+"px;" +
      "font:700 "+size+"px/1 'Rajdhani', Arial, sans-serif;" +
      "color:"+color+";" +
      "text-shadow:" +
        "0 0 6px rgba(255,190,210,.8)," +
        "0 0 12px rgba(255,220,230,.7)," +
        "0 0 18px rgba(255,255,255,.6);" +
      "opacity:1;" +
      "transform:translateY(0);" +
      "transition:opacity 1.4s ease, transform 1.4s ease;";

    layer.appendChild(el);

    requestAnimationFrame(function(){
      el.style.opacity = "0";
      el.style.transform = "translateY(-24px)";
    });

    setTimeout(function(){
      el.remove();
    }, 1500);
  }

  // follow cursor (slower, readable trail)
  window.addEventListener("pointermove", function(e){
    spawn(e.clientX, e.clientY);
  }, { passive:true });

  // gentle burst on click
  window.addEventListener("pointerdown", function(e){
    for (var i=0;i<5;i++){
      spawn(
        e.clientX + (Math.random()*20 - 10),
        e.clientY + (Math.random()*20 - 10)
      );
    }
  }, { passive:true });

})();
</script>


</body>
</html>
