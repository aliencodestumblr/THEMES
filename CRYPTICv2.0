<script>

$(document).ready(function(){

  var accentColor = '#acd7bb';   
  var playerHeight = 116;       

  $('.soundcloud_audio_player').each(function(){

    let baseSrc = $(this).attr('src').split('&')[0];

    let params = [
      'liking=false',
      'sharing=false',
      'auto_play=false',
      'show_comments=false',
      'continuous_play=false',
      'buying=false',
      'show_playcount=false',
      'show_artwork=false',
      'hide_related=true',
      'origin=tumblr',
      'color=' + accentColor.replace('#','')
    ].join('&');

    $(this).attr({
      src: baseSrc + '&' + params,
      height: playerHeight,
      width: '100%',
      loading: 'lazy'
    });

  });

});

</script>

<script>
(function(){
  var t;
  window.addEventListener('scroll', function(){
    document.documentElement.classList.add('is-scrolling');
    clearTimeout(t);
    t = setTimeout(function(){
      document.documentElement.classList.remove('is-scrolling');
    }, 500);
  }, {passive:true});
})();
</script>

<script>
(function(){
  const startBtn = document.getElementById("startBtn");
  const startMenu = document.getElementById("startMenu");
  const descBtn = document.getElementById("descBtn");
  const descWindow = document.getElementById("descWindow");

  function closeAll(){
    startMenu.classList.remove("active");
    descWindow.classList.remove("active");
  }

  function toggleStart(e){
    e.stopPropagation();
    const willOpen = !startMenu.classList.contains("active");
    closeAll();
    if(willOpen) startMenu.classList.add("active");
  }

  function toggleDesc(e){
    e.stopPropagation();

    if(!startMenu.classList.contains("active")){
      startMenu.classList.add("active");
    }
    descWindow.classList.toggle("active");
  }

  startBtn.addEventListener("click", toggleStart);
  descBtn.addEventListener("click", toggleDesc);

  document.addEventListener("click", closeAll);

  startMenu.addEventListener("click", function(e){ e.stopPropagation(); });
  descWindow.addEventListener("click", function(e){ e.stopPropagation(); });

  document.addEventListener("keydown", function(e){
    if(e.key === "Escape") closeAll();
  });
})();
</script>

