}

:root{
  --neoWrapMax: 2600px;   
  --neoGutter: 25px;      
  --neoTop: 50px;         
  --neoEdge: 10px;       
}

.entry{
  width: 100%;
  max-width: var(--neoWrapMax);
  margin: 0 auto;
  height: auto;
}

#posts{
  width: 100%;
  box-sizing: border-box;
  margin: var(--neoTop) auto 50px auto;

  padding-left:  calc(var(--neoEdge) + (var(--neoGutter) / 2));
  padding-right: calc(var(--neoEdge) + (var(--neoGutter) / 2));

  margin-left: 0;
  margin-right: 0;

  top:auto; left:auto; right:auto;
}

.posts{
  float: left;
  box-sizing: border-box;

  width: calc(100% / 5);

  padding-left:  calc(var(--neoGutter) / 2);
  padding-right: calc(var(--neoGutter) / 2);
  padding-bottom:10px;

  margin: 0;
  min-width: 0;  
}

#posts::after{
  content:"";
  display:block;
  clear:both;
}

.posts img,
.posts video,
.posts iframe,
.posts embed,
.posts object,
.posts .tumblr_video_container{
  max-width: 100% !important;
  height: auto !important;
  display: block;
}
.posts iframe,
.posts embed,
.posts object{
  width: 100% !important;
}

{block:PermalinkPage}
