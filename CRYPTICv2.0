/* chat */
.chatitle { 
    margin-top:20px;
    margin-bottom:-11px;
    margin-left:20px;
    margin-right:20px; 
    width:auto; 
    color:{color:accent};
    font-size:9px;
    letter-spacing:1.5px;
    text-transform:uppercase;
    text-align:center; 
    font-weight:bold; 
    z-index:9999999999;
}  

.chat ul {
    margin-top:-1px; 
    margin-bottom:10px;
    margin-left:41px;
    margin-right:10px; 
    padding:10px;
}

.chat li {  
    padding:10px; 
    list-style-type:none;
}

span.chats { 
    color:{color:text}; 
    font-size:9px;
    letter-spacing:.7px; 
    font-weight:bold;
    text-transform:uppercase; 
    letter-spacing:.1em;
}

.chat li:nth-child(odd) {
    border-left:1px solid {color:borders}; 
    border-bottom:1px solid {color:borders};
}

.chat li:nth-child(even) {
    border-right:1px solid {color:borders};
    border-bottom:1px solid {color:borders};
}

.chat li:nth-child(odd):hover {
    border-left:1px solid {color:borders}; 
    border-bottom:1px solid {color:borders}; 
    -webkit-transition-duration:600ms;
    -o-transition-duration:600ms; 
    -moz-transition-duration:600ms; 
    -ms-transition-duration:600ms;
}

.chat li:nth-child(even):hover {
    border-right:1px solid {color:borders};
    border-bottom:1px solid {color:borders}; 
    -webkit-transition-duration:600ms; 
    -o-transition-duration:600ms; 
    -moz-transition-duration:600ms; 
    -ms-transition-duration:600ms;
}
/* end chat */

