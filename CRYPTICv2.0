<script src="https://static.tumblr.com/wgg6svp/OoTofxa0c/unnest.min.js"></script>

<script>
function setupNarniaGrid() {
  var $window    = $(window);
  var $container = $('#posts');
  var $items     = $container.find('.posts');

  if (!$container.length || !$items.length) return;

  // 3 / 4 / 5 from your Tumblr select
  var numColumns = 5;
  var gutter     = 10; // px gap between columns

  // total width we use for the grid = viewport minus 10px left & right
  var viewportWidth = $window.width();
  var available     = Math.max(0, viewportWidth - 20); // safety

  // column width so N columns + gutters exactly fill "available"
  var columnWidth = Math.floor(
    (available - gutter * (numColumns - 1)) / numColumns
  );

  // final grid width is exactly "available"
  var gridWidth = available;

  $container.css({
    width:  gridWidth + 'px',
    margin: '0 auto'
  });

  $items.css({
    width:        columnWidth + 'px',
    marginBottom: gutter + 'px'
  });

  // save numbers for later (used when new posts load)
  $container.data('narniaColumnWidth', columnWidth);
  $container.data('narniaGutter', gutter);

  // reset Masonry if it already exists
  if ($container.data('masonry')) {
    $container.masonry('destroy');
  }

  $container.masonry({
    itemSelector:    '.posts',
    columnWidth:     columnWidth,
    gutter:          gutter,
    isFitWidth:      true,
    percentPosition: false
  });
}

$(document).ready(function () {
  // photosets
  $('.photo-slideshow').pxuPhotoset({
    lightbox: true,
    rounded: false,
    gutter: '1px',
    photoset: '.photo-slideshow',
    photoWrap: '.photo-data',
    photo: '.pxu-photo'
  });

  // unnest captions
  $('.posts').unnest({
    yourCaption: ".caption",
    wrapName: ".tumblr_parent",
    newCaptionUsername: true,
    originalPostCaptionUsername: true,
    tumblrAvatars: false,
    tumblrAvatarsName: ".tumblr_avatar",
    usernameColon: false
  });

  {block:IndexPage}
  var $container = $('#posts');

  // initial layout when images are loaded
  $container.imagesLoaded(function () {
    setupNarniaGrid();
    $container.find('.posts').animate({ opacity: 1, zIndex: 1 });
  });

  // re-layout on resize
  $(window).on('resize', function () {
    setupNarniaGrid();
  });

  // infinite scroll
  $container.infinitescroll({
      itemSelector: '.posts',
      navSelector: '#pagination',
      nextSelector: '#next',
      loadingImg: '',
      loadingText: '<em></em>',
      bufferPx: 1000
    },
    function (newElements) {
      var $newElems = $(newElements).css({ opacity: 0 });

      // unnest captions for new posts
      $newElems.unnest({
        yourCaption: ".caption",
        wrapName: ".tumblr_parent",
        newCaptionUsername: true,
        originalPostCaptionUsername: true,
        tumblrAvatars: false,
        tumblrAvatarsName: ".tumblr_avatar",
        usernameColon: false
      });

      // update like buttons
      var $newElemsIDs = $newElems.map(function () {
        return this.id;
      }).get();
      if (Tumblr && Tumblr.LikeButton && Tumblr.LikeButton.get_status_by_post_ids) {
        Tumblr.LikeButton.get_status_by_post_ids($newElemsIDs);
      }

      // photosets inside new posts
      $newElems.find('.photo-slideshow').pxuPhotoset({
        lightbox: true,
        rounded: false,
        gutter: '1px',
        photoset: '.photo-slideshow',
        photoWrap: '.photo-data',
        photo: '.pxu-photo'
      });

      // once new images are loaded, append then relayout grid
      $newElems.imagesLoaded(function () {
        // use the same width/gutter as existing columns
        var columnWidth = $container.data('narniaColumnWidth') || $container.find('.posts').outerWidth();
        var gutter      = $container.data('narniaGutter') || 10;

        $newElems.css({
          width:        columnWidth + 'px',
          marginBottom: gutter + 'px'
        });

        // append + tell Masonry about the new items (no destroy)
        $container.append($newElems);
        $container.masonry('appended', $newElems);

        $newElems.animate({ opacity: 1, zIndex: 1 });

        if (typeof resizeVideos === 'function') {
          resizeVideos();
        }
      });
    }
  );
  {/block:IndexPage}
});
</script>

  
