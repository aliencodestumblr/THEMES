.caption p {
  margin-top: 0px !important;
  line-height: 1.3 !important;
}

p > a.tumblr_blog {
  display: none !important;
}

p > a.tumblr_blog::after {
  content: none !important;
}

p:has(> a.tumblr_blog) {
  display: none !important;
}

a.tumblr_blog {
  display: none !important;
}  

#s-m-t-tooltip {
  position: absolute;
  max-width: 280px;
  padding: 4px 5px 4px 6px;
  margin-top: 14px;
  border-radius: 6px;
  background: #000; 
  font-family: "Rajdhani", sans-serif;
  font-size: 10px;
  text-transform: uppercase;
  letter-spacing: 1.5px;
  color: {color:tooltip text}; 
  opacity: 0;
  z-index: 99999;
  pointer-events: none;
  transform: translateY(6px) scale(0.96);
  transition: opacity .25s ease, transform .25s ease, box-shadow .25s ease;
  box-shadow: 
    0 0 0 1px rgba(255,255,255,0.06),
    0 6px 18px rgba(0,0,0,0.55),
    0 0 12px rgba(255,255,255,0.04);
}

#s-m-t-tooltip.show {
  opacity: 1;
  transform: translateY(0) scale(1);
  box-shadow: 
    0 0 0 1px rgba(255,255,255,0.08),
    0 8px 24px rgba(0,0,0,0.65),
    0 0 18px {color:Link};
}

:root{
  --tbH: 42px;
  --tbBorder: 1px;
  --tbR: 6px;      
  --tbPadX: 12px;

  --tbBg: rgba(0,0,0,.72);
  --tbText: rgba(255,255,255,.70);
  --tbTextStrong: rgba(255,255,255,.92);
  --tbBorderCol: rgba(255,255,255,.12);
  --tbHover: rgba(255,255,255,.08);
  --tbPanelBg: rgba(0,0,0,.78);
  --tbPanelBorder: rgba(255,255,255,.14);
  --tbShadow: 0 14px 38px rgba(0,0,0,.35);
}

/* Apply mood via class output from {select:Taskbar Mood} */
.tb-dark{
  --tbBg: rgba(0,0,0,.90);
  --tbText: rgba(255,255,255,.70);
  --tbTextStrong: rgba(255,255,255,.92);
  --tbBorderCol: rgba(255,255,255,.12);
  --tbHover: rgba(255,255,255,.08);
  --tbPanelBg: rgba(0,0,0,.90);
  --tbPanelBorder: rgba(255,255,255,.14);
  --tbShadow: 0 14px 38px rgba(0,0,0,.35);
}

.tb-light{
  --tbBg: rgba(255,255,255,.90);
  --tbText: rgba(0,0,0,.75);
  --tbTextStrong: rgba(0,0,0,.92);
  --tbBorderCol: rgba(0,0,0,.12);
  --tbHover: rgba(0,0,0,.06);
  --tbPanelBg: rgba(255,255,255,.90);
  --tbPanelBorder: rgba(0,0,0,.14);
  --tbShadow: 0 14px 38px rgba(0,0,0,.18);
}

body{
  padding-bottom: calc(var(--tbH) + 18px);
}

.taskbar{
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: var(--tbH);
  z-index: 999999;

  display: flex;
  align-items: center;
  gap: 10px;

  font-family: "Rajdhani", sans-serif;

  background: var(--tbBg);
  -webkit-backdrop-filter: blur(12px);
  backdrop-filter: blur(12px);

  border-top: var(--tbBorder) solid var(--tbBorderCol);
}

.tb-left{
  position: relative;
  display: flex;
  align-items: center;
  height: 100%;
}

.start-btn{
  height: 100%;
  padding: 0 16px;

  display: flex;
  align-items: center;

  font-size: 11px;
  text-transform: uppercase;
  letter-spacing: 1.6px;

  cursor: pointer;
  user-select: none;

  color: var(--tbTextStrong);

  border-right: var(--tbBorder) solid var(--tbBorderCol);
  transition: background .16s ease, color .16s ease;
}

.start-btn:hover{
  background: var(--tbHover);
}

.start-menu{
  position: absolute;
  left: 0;
  bottom: var(--tbH);

  width: 220px;

  background: var(--tbPanelBg);
  -webkit-backdrop-filter: blur(12px);
  backdrop-filter: blur(12px);

  border: 1px solid var(--tbPanelBorder);
  border-bottom: none;

  padding: 8px 0;

  display: none;
  flex-direction: column;

  box-shadow: var(--tbShadow);
}

.start-menu.active{ display:flex; }

.start-menu a,
.start-menu .menu-item{
  padding: 9px 14px;

  font-size: 11px;
  text-transform: uppercase;
  letter-spacing: 1.4px;

  color: var(--tbText);
  cursor: pointer;
  text-decoration: none;

  transition: background .16s ease, color .16s ease;
}

.start-menu a:hover,
.start-menu .menu-item:hover{
  background: var(--tbHover);
  color: var(--tbTextStrong);
}

.desc-window{
  position: absolute;
  bottom: var(--tbH);
  left: 221px;

  width: 270px;
  max-height: 240px;
  overflow: auto;

  background: var(--tbPanelBg);
  -webkit-backdrop-filter: blur(12px);
  backdrop-filter: blur(12px);

  border: 1px solid var(--tbPanelBorder);
  border-bottom: none;

  padding: 14px;

  font-size: 11px;
  line-height: 1.5;
  color: var(--tbText);
  display: none;

  box-shadow: var(--tbShadow);  
}

.desc-window.active{ display:block; }

.taskbar-right{
  margin-left: auto;
  height: 100%;
  padding: 0 var(--tbPadX);

  display: flex;
  align-items: center;
  gap: 10px;
}

.tb-title{
  font-size: 8px;
  letter-spacing: 4px;
  color: var(--tbTextStrong);
  text-transform:uppercase;
  white-space: nowrap;
  max-width: 240px;
  font-weight:100;
  overflow: hidden;
  text-overflow: ellipsis;
}

.tb-avatar{
  width: 28px;
  height: 28px;

  border-radius: var(--tbR);
  overflow: hidden;
  flex: 0 0 auto;

  border: 1px solid var(--tbBorderCol);
  display: block;
}

.tb-avatar img{
  width: 100%;
  height: 100%;
  object-fit: cover;
  display: block;
}

.neoPageTitle{
  width: auto;
  margin: 30px auto 40px auto;
  
  display: flex;
  justify-content: center;

  text-align: center;
  transform: none !important;
  position: relative;
  left: 0 !important;
}

.neoPageTitle h1{
  margin: 0;
  color:{color:Blog Title};
  font-size: 18px;
  font-weight: 800;
  text-transform: uppercase;
  letter-spacing: .35em;

  width: 100%;
  text-align: center;
}

#posts{
  padding-left:  calc(var(--neoEdge) + (var(--neoGutter) / 2)) !important;
  padding-right: calc(var(--neoEdge) + (var(--neoGutter) / 2)) !important;
  margin-left: 0 !important;
  margin-right: 0 !important;
}

#posts{
  transform: translateX(8px);
}

iframe.ask_form,
iframe[src*="tumblr.com/ask_form"],
iframe[src*="/ask_form"]{
  width: 100% !important;
  display: block !important;
  border: 0 !important;

  min-height:auto !important;
  height: 23vh !important;
  max-height: none !important;
}

body{
  padding-bottom: calc(var(--tbH) + 80px + env(safe-area-inset-bottom)) !important;
}

{block:AskPage}
#posts{ padding-bottom: 220px !important; }
{/block:AskPage}
