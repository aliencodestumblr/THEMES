/* scrollbar */
::-webkit-scrollbar{
  width:8px;
}

::-webkit-scrollbar-track{
  background: transparent;
}

::-webkit-scrollbar-thumb{
  background: transparent;     
  border-radius: 3px;
}

.is-scrolling ::-webkit-scrollbar-thumb{
  background: {color:Accent};
}

.is-scrolling ::-webkit-scrollbar-thumb:hover{
  box-shadow: 0 0 6px rgba({RGBcolor:Accent}, .45);
}

*{
  scrollbar-width: thin;
  scrollbar-color: transparent transparent;
}
.is-scrolling{
  scrollbar-color: {color:Accent} transparent;
}
/* end scrollbar */


/* pagination */
.pag {      
    bottom:40px;
    margin-left:0px;
    margin-bottom:20px; 
    width:100%;
    height:auto;  
    text-align:center;   
    z-index:100;
}

.pag a{ 
    background:{color:background};
    width:auto; 
    font-size:12px;  
    text-align:center;
    text-transform:uppercase; 
    color:{color:accent};    
    padding:2px 7px 3px 7px;     
    margin:0px; 
    transition:1s;
    border-radius:3px;
}

.pag a:hover{
    color:{color:accent};
    font-weight:700; 
    transition:1s;
}
/* end pagination */

/* notes */ 

.post-footer{
  margin-top:0px;
  padding-top:6px;
  
  display:flex;
  justify-content:flex-end;
  align-items:center;
  gap:8px;
  margin-right:-4px;
}

.pf-notes{
  display:inline-flex;
  align-items:baseline;
  gap:6px;

  text-decoration:none;
  color:{color:text};
  opacity:.62;
  
  font-family:"Rajdhani", sans-serif;
  font-size:10px;
  letter-spacing:1.2px;
  text-transform:lowercase;

  transition:opacity .15s ease, color .15s ease;
}

.pf-notes .pf-num{
  font-weight:600;
  letter-spacing:.6px;
}

.pf-notes .pf-lab{
  opacity:.75;
}

.pf-btn{
  width:18px;
  height:18px;

  display:inline-flex;
  align-items:center;
  justify-content:center;

  text-decoration:none;
  color:{color:text};
  opacity:.55;

  transition:opacity .15s ease, color .15s ease;
}

.pf-btn svg{
  width:12px;
  height:12px;
  display:block;
}

.pf-notes:hover,
.pf-btn:hover{
  opacity:1;
  color:{color:Accent};
}

/* end notes */

/* permalink */
.perma {  
    margin-top:10px;
    margin-left:-28px;  
    margin-bottom:100px;
    width:700px;
    height:auto; 
    overflow:auto;   
    font-family:"Rajdhani", sans-serif;
    text-align:left; 
    text-transform:uppercase; 
    font-style:normal;   
    padding:0px;    
}

ol.notes {    
    margin-top:0px;
    margin-left:0px; 
    margin-right:0px;  
    width:auto;
    height:auto;  
    padding:1px;
    font-size:8.5px;
    text-align:left; 
    text-transform:uppercase; 
    letter-spacing:1px; 
    line-height:17px;      
    overflow:auto;
} 

ol.notes li.note {  
    padding:3px 5px 2px 5px;   
    border-radius:5px;
    margin:3px;
}

li.note {
    display:block;
}

ol.notes li a { 
    font-weight:700;
} 

ol.notes li.like:before { 
    margin-top:5px;  
    margin-left:0px;
    margin-right:5px; 
    font-family:'FontAwesome';  
    content: "\f004";     
    padding:6px 2px 5px 6px;
    border-radius:100%;
}

ol.notes li.reblog:before {  
    margin-top:4px;  
    margin-left:0px;
    margin-right:5px; 
    font-family:'FontAwesome';  
    content: "\f061";  
    padding:6px 2px 6px 6px;
    border-radius:100%;
} 

ol.notes li.original_post:before {  
    margin-top:4px;  
    margin-left:0px;
    margin-right:5px; 
    font-family:'FontAwesome';  
    content: "\f124";  
    padding:6px 2px 6px 6px;
    border-radius:100%;
} 

ol.notes li.reply:before {  
    margin-top:4px;  
    margin-left:0px;
    margin-right:5px; 
    font-family:'FontAwesome';  
    content: "\f044";  
    padding:6px 2px 6px 6px;
    border-radius:100%;
} 

ol.notes li.note img.avatar {
    display:none;
}  

.perma-info {
    margin-top:10px;
    margin-left:0px;
    margin-right:5px;
    margin-bottom:10px;
    font-size:9px;
    text-transform:uppercase;
    letter-spacing:0.5px; 
}
/* end permalink */


/* audios */
.album {
  margin-left: 20px;
}

.player {
  opacity: 1;
  pointer-events: auto;
  margin: 0;
}

.art,
.track {
  display: none;
}
