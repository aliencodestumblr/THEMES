<style type="text/css">
body { overflow-y: scroll; } 

:root{
  --sp-scale: 0.80;   
  --sp-base-height: 80px;  
}

article iframe[src*="spotify.com"],
iframe.spotify_audio_player {
  aspect-ratio: auto !important;
}

article iframe[src*="spotify.com"],
iframe.spotify_audio_player {
  width: calc(100% / var(--sp-scale)) !important;
  height: var(--sp-base-height) !important; 
  transform: scale(var(--sp-scale)) !important;
  transform-origin: top left !important;
  display: block;
  border: 0;
}

figure iframe.spotify_audio_player {
  max-height: none !important; 
  width: calc(100% / var(--sp-scale)) !important;
}

article iframe[src*="spotify.com"],
iframe.spotify_audio_player {
  margin-bottom: calc(var(--sp-base-height) * (var(--sp-scale) - 1)) !important; 
  display: block;
}

.cc{
  position: fixed;
  top: 20px;
  right: 20px;
  width: 27px;
  height: 27px;

  background: var(--tbBg);
  -webkit-backdrop-filter: blur(12px);
  backdrop-filter: blur(12px);

  border: 1px solid var(--tbBorderCol);
  border-radius: 5px;
  
  
  display: flex;
  align-items: center;
  justify-content: center;

  z-index: 100;
  padding: 0;
  color:{color:accent};
  transition: transform 0.3s ease, box-shadow 0.3s ease, background .25s ease, border-color .25s ease;
}

.cc:hover{
  background: var(--tbHover);
  transform: translateY(-1px);
  box-shadow: var(--tbShadow);
}

.cc svg {
    display: block;
    width: 14px;
    height: 14px;
    overflow: visible;
}

.cc svg path {
   fill: currentColor;
}
     
iframe.tmblr-iframe {
    z-index: 999;
    right: 20px;
    top: 18px;
    opacity: 0;
    position: fixed;
    width: 0;
    {block:IfDarkTumblrControl}
    filter:invert(1);
    {/block:IfDarkTumblrControl}
    padding-right: 60px;
    transform: scale(0.6);
    -webkit-transform: scale(0.6);
    transform-origin: 100% 0;
    -webkit-transform-origin: 100% 0;
    transition: opacity 0.4s ease;
    -webkit-transition: opacity 0.4s ease;
}

.tmblr-iframe:hover {
    opacity: 0.5 !important;
}

html, body, a, a:hover{cursor:crosshair;}
 
@keyframes fadein {from {opacity:0;} to {opacity:1;}} 
@-moz-keyframes fadein {from {opacity:0;} to {opacity:1;}} /* Firefox */
@-webkit-keyframes fadein {from {opacity:0;} to {opacity:1;}} /* Safari and Chrome */
@-o-keyframes fadein {from {opacity:0;} to {opacity:1;}} /* Opera */

.vignette, #vignette {
    opacity:0;
}
.lightbox-image, #tumblr_lightbox img {  
    box-shadow:none !important;
    border-radius:2px !important;
    max-width:none;
}
 
.tmblr-lightbox, #tumblr_lightbox {
    background-color:rgba({RGBcolor:background},.7) !important;
}
 
.lightbox-image, #tumblr_lightbox img {  
    border:5px solid {color:background}!important;
    box-shadow: 1px 1px 2px rgba(0,0,0,.1);
    -moz-box-sizing:border-box;
    box-sizing:border-box;
}

hr {
    margin:10px auto;
    border-style:solid;
    border-color:{color:borders};
    border-top-width:0.5px;
    border-bottom-width:0.5px;
}

pre, code {
    overflow:hidden;
    overflow-y:hidden;
    overflow-x:scroll; 
    user-select:all; 
