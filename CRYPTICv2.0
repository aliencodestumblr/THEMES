}
::selection {background:{color:background};
    color:{color:links hover};
}
 
.tumblrAutoPager_page_separator, .tumblrAutoPager_page_info {
     display:none;
}

body {
    margin:0;
    margin-right:0px;
    color:{color:text}; 
    background-color:{color:background};
