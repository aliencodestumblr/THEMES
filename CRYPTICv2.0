/* quote */
.quote {    
    text-align:justify;
    line-height:15px;  
    margin-bottom:-5px;
    margin-top:0px;  
    margin-left:0px;
    margin-right:0px; 
    padding:20px; 
}

.quote:first-letter {
    text-transform:uppercase;
}
 
.sourceqt {  
    margin-top:0px;
    margin-left:20px; 
    margin-right:20px;  
    margin-bottom:20px;
    color:{color:links};
    width:auto;  
    text-align:justify;  
    text-transform:normal;
    font-weight:bold; 
