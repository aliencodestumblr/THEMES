    {block:PreviousPage}
        <a href="{PreviousPage}">←</a>
    {/block:PreviousPage}

    {block:JumpPagination length="5"}
        {block:CurrentPage}
            <span class="current_page">{PageNumber}</span>
        {/block:CurrentPage}
        {block:JumpPage}
            <a href="{URL}">{PageNumber}</a>
        {/block:JumpPage}
    {/block:JumpPagination}

