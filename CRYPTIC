<script>

$(document).ready(function(){

  var accentColor = '#acd7bb';   
  var playerHeight = 116;       

  $('.soundcloud_audio_player').each(function(){

    let baseSrc = $(this).attr('src').split('&')[0];

    let params = [
      'liking=false',
      'sharing=false',
      'auto_play=false',
      'show_comments=false',
      'continuous_play=false',
      'buying=false',
      'show_playcount=false',
      'show_artwork=false',
      'hide_related=true',
      'origin=tumblr',
      'color=' + accentColor.replace('#','')
    ].join('&');

    $(this).attr({
      src: baseSrc + '&' + params,
      height: playerHeight,
      width: '100%',
      loading: 'lazy'
    });

  });

});

</script>

<script>
function toggleStart(e) {
  e.stopPropagation();
  document.getElementById("startMenu").classList.toggle("active");
}

function toggleDesc(e) {
  e.stopPropagation();
  document.getElementById("descWindow").classList.toggle("active");
}

document.addEventListener("click", function () {
  document.getElementById("startMenu").classList.remove("active");
  document.getElementById("descWindow").classList.remove("active");
});
</script>

<script>
document.addEventListener("DOMContentLoaded", function () {

  const startBtn  = document.querySelector(".start-btn");
  const startMenu = document.querySelector(".start-menu");

  startBtn.addEventListener("click", function (e) {
    e.stopPropagation();
    startMenu.classList.toggle("active");
  });

  startMenu.addEventListener("click", function (e) {
    e.stopPropagation();
  });

  document.addEventListener("click", function () {
    startMenu.classList.remove("active");
  });

});
</script>

<script>feather.replace()</script>
