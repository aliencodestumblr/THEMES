<script src="https://static.tumblr.com/wgg6svp/OoTofxa0c/unnest.min.js"></script>

<script>
$(document).ready(function(){
$('.photo-slideshow').pxuPhotoset({
lightbox: true,
rounded: false,
gutter: '1px',
photoset: '.photo-slideshow',
photoWrap: '.photo-data',
photo: '.pxu-photo'
});
$(function(){ $('.posts').unnest({ 
yourCaption:".caption",
wrapName:".tumblr_parent",
newCaptionUsername:true,
originalPostCaptionUsername:true, 
tumblrAvatars:false,  
tumblrAvatarsName:".tumblr_avatar",  
usernameColon:false 
});}) 
{block:IndexPage}
var $container = $('#posts');
$container.masonry({ itemSelector: '.posts' });
$container.imagesLoaded(function(){
$container.masonry();
$container.find('.posts').animate({ opacity: 1, zIndex: 1 });
}); 
$container.infinitescroll({
itemSelector: '.posts',
navSelector: '#pagination',
nextSelector: '#next',
loadingImg: '',
loadingText: '<em></em>',
bufferPx: 1000
},
function(newElements) {
var $newElems = $( newElements ).css({ opacity: 0 });
$newElems.unnest({
yourCaption:".caption",
wrapName:".tumblr_parent",
newCaptionUsername:true,
originalPostCaptionUsername:true, 
tumblrAvatars:false,
tumblrAvatarsName:".tumblr_avatar",  
usernameColon:false });
var $newElemsIDs = $newElems.map(function(){
return this.id;
}).get();
Tumblr.LikeButton.get_status_by_post_ids($newElemsIDs);
var $newElems = $( newElements );
$newElems.find('.photo-slideshow').pxuPhotoset({
lightbox: true,
rounded: false,
gutter: '1px',
photoset: '.photo-slideshow',
photoWrap: '.photo-data',
photo: '.pxu-photo'
},
function(){
$container.masonry();
});
$newElems.imagesLoaded(function(){
$container.masonry( 'appended', $newElems );
$newElems.animate({ opacity: 1, zIndex: 1 });
resizeVideos();
});
}); 
{/block:IndexPage}
});
</script>
  
{block:ifnotpagination}
  <div id="pagination" class="infinite-scroll">
    {block:NextPage}
      <a id="next" href="{NextPage}"></a>
    {/block:NextPage}
  </div>
{/block:ifnotpagination}

{block:ifPagination}
	<nav id="pagination">
        <span class="current_page">page {CurrentPage} of {totalpages}</span>
        
    	<div>
            <a {block:PreviousPage}class="active" href="{PreviousPage}"{/block:PreviousPage}>
                backward
            </a>
 
            <a {block:NextPage}class="active" href="{NextPage}"{/block:NextPage}>
                forward
            </a>
    	</div>
	</nav>
{/block:ifPagination}

{block:DayPagination}
    <nav id="pagination">
    	<div>
            <a {block:PreviousPage}class="active" href="{PreviousPage}"{/block:PreviousPage}>
                backward
            </a>
 
            <a {block:NextPage}class="active" href="{NextPage}"{/block:NextPage}>
                forward
            </a>
    	</div>
        
	</nav>
{/block:DayPagination}
</div> 
</div>

<script>
(function () {
  var toggle = document.getElementById("menu-toggle");
  var dropdown = document.getElementById("header-dropdown");

  if (!toggle || !dropdown) return;

  toggle.addEventListener("click", function (e) {
    e.stopPropagation();
    dropdown.classList.toggle("is-open");
  });

  document.addEventListener("click", function () {
    dropdown.classList.remove("is-open");
  });

  dropdown.addEventListener("click", function (e) {
    e.stopPropagation();
  });
})();
</script>

<script>
const descToggle = document.getElementById("header-desc-toggle");
const descDropdown = document.getElementById("desc-dropdown");

descToggle.addEventListener("click", () => {
  descDropdown.classList.toggle("is-open");
});
</script>
