<script>

$(document).ready(function(){

  var accentColor = '#acd7bb';   
  var playerHeight = 116;       

  $('.soundcloud_audio_player').each(function(){

    let baseSrc = $(this).attr('src').split('&')[0];

    let params = [
      'liking=false',
      'sharing=false',
      'auto_play=false',
      'show_comments=false',
      'continuous_play=false',
      'buying=false',
      'show_playcount=false',
      'show_artwork=false',
      'hide_related=true',
      'origin=tumblr',
      'color=' + accentColor.replace('#','')
    ].join('&');

    $(this).attr({
      src: baseSrc + '&' + params,
      height: playerHeight,
      width: '100%',
      loading: 'lazy'
    });

  });

});

</script>

<script>feather.replace()</script>

<script>
document.addEventListener("DOMContentLoaded", function() {
  const prev = document.getElementById("prev-page");
  const next = document.getElementById("next-page");

  const prevLink = document.querySelector(".pagination a[href*='page=']") || null;
  const nextLink = document.querySelector(".pagination a[href*='page=']") || null;

  if(prevLink) {
    prev.href = prevLink.href;
  } else {
    prev.href = window.location.href; 
  }

  if(nextLink) {
    next.href = nextLink.href;
  } else {
    next.href = window.location.href; 
  }
});
</script>

