<script>

$(document).ready(function(){

  var accentColor = '#acd7bb';   
  var playerHeight = 116;       

  $('.soundcloud_audio_player').each(function(){

    let baseSrc = $(this).attr('src').split('&')[0];

    let params = [
      'liking=false',
      'sharing=false',
      'auto_play=false',
      'show_comments=false',
      'continuous_play=false',
      'buying=false',
      'show_playcount=false',
      'show_artwork=false',
      'hide_related=true',
      'origin=tumblr',
      'color=' + accentColor.replace('#','')
    ].join('&');

    $(this).attr({
      src: baseSrc + '&' + params,
      height: playerHeight,
      width: '100%',
      loading: 'lazy'
    });

  });

});

</script>

<script>
(function(){
  var audio = document.getElementById('audio');
  if(!audio) return;

  var wrap = document.querySelector('#sidebar .sb-player');
  if(!wrap) return;

  var playBtn = wrap.querySelector('.sb-play');
  var pauseBtn = wrap.querySelector('.sb-pause');

  function setState(playing){
    if(playBtn) playBtn.style.display = playing ? 'none' : 'inline-flex';
    if(pauseBtn) pauseBtn.style.display = playing ? 'inline-flex' : 'none';
  }

  if(playBtn){
    playBtn.addEventListener('click', function(e){
      e.preventDefault();
      audio.play();
    });
  }

  if(pauseBtn){
    pauseBtn.addEventListener('click', function(e){
      e.preventDefault();
      audio.pause();
    });
  }

  audio.addEventListener('play', function(){ setState(true); });
  audio.addEventListener('pause', function(){ setState(false); });
  audio.addEventListener('ended', function(){ setState(false); });

  setState(!audio.paused);
})();
</script>

<script>
(function(){
  var t;
  window.addEventListener('scroll', function(){
    document.documentElement.classList.add('is-scrolling');
    clearTimeout(t);
    t = setTimeout(function(){
      document.documentElement.classList.remove('is-scrolling');
    }, 500);
  }, {passive:true});
})();
</script>

