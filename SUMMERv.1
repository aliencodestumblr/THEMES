    margin-left:12px;  
    margin-bottom:100px;
    width:540px;
    height:auto; 
    overflow:auto;   
    font-family:"Rajdhani", sans-serif;
    letter-spacing:1px;
    text-align:left; 
    text-transform:uppercase; 
    font-style:normal;   
    padding:0px;    
}

ol.notes {    
    margin-top:0px;
    margin-left:0px; 
    margin-right:0px;  
    width:auto;
    height:auto;  
    padding:1px;
    font-size:8.5px;
    text-align:left; 
    text-transform:uppercase; 
    letter-spacing:1px; 
    line-height:17px;      
    overflow:auto;
} 

ol.notes li.note {  
    background:{color:details};
    padding:3px 5px 2px 5px;   
    border-radius:5px;
    border:0px solid {color:borders};
    margin:3px;
}

li.note {
    display:block;
}

ol.notes li a { 
    font-weight:700;
} 

ol.notes li.like:before { 
    margin-top:5px;  
    margin-left:0px;
    margin-right:5px; 
    font-family:'FontAwesome';  
    content: "\f004";     
    padding:6px 2px 5px 6px;
    border-radius:100%;
}

ol.notes li.reblog:before {  
    margin-top:4px;  
    margin-left:0px;
    margin-right:5px; 
    font-family:'FontAwesome';  
    content: "\f061";  
    padding:6px 2px 6px 6px;
    border-radius:100%;
} 

ol.notes li.original_post:before {  
    margin-top:4px;  
    margin-left:0px;
    margin-right:5px; 
    font-family:'FontAwesome';  
