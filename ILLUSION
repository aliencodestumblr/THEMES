<script>feather.replace()</script>

<script>
  document.addEventListener('DOMContentLoaded', function() {
    // Find all source links inside your "second row" div
    var sourceLinks = document.querySelectorAll('div[style*="font-size:9px"] a');

    sourceLinks.forEach(function(link) {
      if (link.href.includes('-deactivated')) {
        // Replace the visible text in the link by removing the "-deactivated" part
        link.textContent = link.textContent.replace('-deactivated', '');
      }
    });
  });
</script>

</body>
</html>
